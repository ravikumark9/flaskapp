{
    "family": "first-run-task-definition", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "cpu": "256", 
            "memory": "512",
            "name": "ecr-ecs", 
            "image": "{{image}}", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "command": ["sleep", "10"],
            "entryPoint": ["/"],
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ]   
}
