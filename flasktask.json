{
    "family": "flasktask",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "flaskapp",
            "image": "{{image}}",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "256"
}
